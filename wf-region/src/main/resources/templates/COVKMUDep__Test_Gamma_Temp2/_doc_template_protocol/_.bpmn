    <scriptTask id="scripttask11" name="Адресаты СЕВ в строку" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>var sName_Table = 'sTableSEV1'; //Название таблицы
var sLogin_Execute = 'sLogin_isExecute'; //ID столбца с логином
var sID_Field = 'sAdresatSEV1'; //поле с списком логинов
var snID_Process_Activiti = execution.getProcessInstanceId();

//Список логинов адресатов
var aLoginDirect = taskForm.getValuesFromTableField(snID_Process_Activiti, sName_Table, sLogin_Execute);
saLoginDirect = aLoginDirect.toString();
sListLogins = saLoginDirect.replace(/\[|\]/g, '');
sListLogins = sListLogins.replace(/\s/g, '');

execution.setVariable(sID_Field, sListLogins);</script>
    </scriptTask>
    <scriptTask id="scripttask12" name="Перебор по индексу" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>var snID_Process_Activiti = execution.getProcessInstanceId();                                                           

//длинна массива
  var aNameOKPO = taskForm.getValuesFromTableField(snID_Process_Activiti, 'sTableSEV1', 'sAutOrgan');
    if (aNameOKPO != null){
        var nRowCount = aNameOKPO.length; 
        nRowCount = nRowCount - 1;
      execution.setVariable('nRowCount', nRowCount.toString()); 
  }
  var nCount = execution.getVariable('nCount');


//массив значений колонки sOKPO
  var aOKPO = taskForm.getValuesFromTableField(snID_Process_Activiti, 'sTableSEV1', 'sOKPO');

    var sListOKPO = aOKPO[nCount];
    execution.setVariable('sListOKPO', sListOKPO);</script>
    </scriptTask>
    <scriptTask id="scripttask13" name="Count +1 (index)" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>var nCount = execution.getVariable('nCount');
	nCount++;
execution.setVariable('nCount', nCount.toString());</script>
    </scriptTask>
