    <scriptTask id="scripttask2" name="Текст листа" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>        
      var Area=' '
      var Posluga =' '
      
        if(execution.getVariable('IDRaion') == 'Шевченківський район')
          {
          Area='м. Дніпро, пр. Богдана Хмельницького, буд. 116А'
          }
        if(execution.getVariable('IDRaion') == 'Центральний район')
          {
          Area='м. Дніпро,  вул. Володимира Антоновича, буд.30/32'
          }
        if(execution.getVariable('IDRaion') == 'Соборний район')
          {
          if(execution.getVariable('IDVikno') == 'Вікно 4')
            {
            Area='м. Дніпро, пр. Героїв буд. 40'
            }
          if(execution.getVariable('IDVikno') == 'Вікно 5')
            {
            Area='м. Дніпро, пр. Героїв буд. 40'
            }
          if(execution.getVariable('IDVikno') == 'Вікно 6')
            {
            Area='м. Дніпро, пр. Героїв буд. 40'
            }
          if(execution.getVariable('IDVikno') == 'Вікно 7')
            {
            Area='м. Дніпро, пр. Героїв буд. 40'
            }
          if(execution.getVariable('IDVikno') == 'Кабінет 8 - Вікно 1')
            {
            Area='м. Дніпро, вул. В. Жуковського буд. 39/41'
            }
          if(execution.getVariable('IDVikno') == 'Кабінет 14 - Вікно 1')
            {
            Area='м. Дніпро, вул. В. Жуковського буд. 39/41'
            }
          if(execution.getVariable('IDVikno') == 'Кабінет 1 - Вікно 1')
            {
            Area='м. Дніпро, вул. В. Жуковського буд. 39/41'
            }
          if(execution.getVariable('IDVikno') == 'Кабінет 14 - Вікно 2')
            {
            Area='м. Дніпро, вул. В. Жуковського буд. 39/41'
            }
          if(execution.getVariable('IDVikno') == 'Кабінет 12 - Вікно 1')
            {
            Area='м. Дніпро, вул. В. Жуковського буд. 39/41'
            }
          if(execution.getVariable('IDVikno') == 'Кабінет 12 - Вікно 2')
            {
            Area='м. Дніпро, вул. В. Жуковського буд. 39/41'
            }
          }
        if(execution.getVariable('IDRaion') == 'Новокодацький район')
          {
          Area='м. Дніпро, вул. Трудова, 1а'
          }
        if(execution.getVariable('IDRaion') == 'Чечелівський район')
          {
          Area='м. Дніпро, вул. Уральська, 5'
          }
        if(execution.getVariable('IDRaion') == 'Амур-Нижньодніпровський район')
          {
          Area='м. Дніпро, вул. Радистів, буд. 8'
          }
        if(execution.getVariable('IDRaion') == 'Індустріальний район')
          {
          Area='м. Дніпро, пр. Петра Калнишевського буд. 55'
          }      
        
        if(execution.getVariable('IDPosluga') == 'Допомога у зв\'язку з вагітністю та пологами')
          {
          Posluga ='Копія реєстраційного номера облікової картки платника податків  (з пред\'явленням оригіналу).&lt;br /&gt;Довідка, видана лікувально-профілактичним закладом, за формою № 147/о.&lt;br /&gt;Довідка з місця навчання про те, що жінка навчається – для студентів.&lt;br /&gt;Довідка ліквідаційної комісії про те, що жінка звільнена з роботи у зв\'язку з ліквідацією підприємства, установи та організації – для жінок, звільнених з роботи.&lt;br /&gt;Довідка центру зайнятості про те, що жінка зареєстрована у центрі зайнятості як безробітна – для жінок, які перебувають на обліку в центрі зайнятості.&lt;br /&gt;Довідка Пенсійного фонду України про те, що жінка, яка є фізичною особою - підприємцем, перебуває (не перебуває) на обліку в органах Пенсійного фонду України та не бере участь на добровільних засадах у загальнообов\'язковому державному соціальному страхуванню у зв\'язку з тимчасовою втратою працездатності та витратами, зумовленими похованням – для жінок, які є фізичними особами – підприємцями.&lt;br /&gt;Особи, які усиновили чи взяли під опіку дитину протягом двох місяців з дня її народження, подають рішення про усиновлення чи встановлення опіки.&lt;br /&gt;Реквізити банківської установи для перерахування коштів.'
          }
        if(execution.getVariable('IDPosluga') == 'Допомога при народженні дитини')
          {
          Posluga ='Копія реєстраційного номера облікової картки платника податків  (з пред\'явленням оригіналу).&lt;br /&gt;Копія свідоцтва про народження дитини (з пред\'явленням оригіналу).&lt;br /&gt;Довідка про неодержання зазначеної допомоги в органах праці та соціального захисту населення за місцем реєстрації (у випадку призначення допомоги за місцем фактичного проживання).&lt;br /&gt;Реквізити банківської установи для перерахування коштів.'
          }
        if(execution.getVariable('IDPosluga') == 'Допомога при усиновленні дитини')
          {
          Posluga ='Копія реєстраційного номера облікової картки платника податків (з пред\'явленням оригіналу).&lt;br /&gt;Копія свідоцтва про народження дитини, виданого державним органом реєстрації актів цивільного стану після внесення змін до актового запису про народження дитини на підставі рішення суду про усиновлення дитини.&lt;br /&gt;Копія рішення суду про усиновлення дитини.&lt;br /&gt;Реквізити банківської установи для перерахування коштів.'
          }          
       if(execution.getVariable('IDPosluga') == 'Надання стипендії особам, яким виповнилось 100 і більше років')
          {
          Posluga ='Відповідно до розпорядження голови Дніпропетровської обласної державної адміністрації від 27.07.2005 № 229-р-05 (із змінами) \"Про щомісячну стипендію особам, яким виповнилось 100 і більше років\" для призначення довічної стипендії особам, яким виповнилось 100 років необхідно звернутися із заявою та додати копію паспорту та індивідуального номера платника податків.&lt;br /&gt;Довічна щомісячна стипендія виплачується за місцем отримання пенсії.'
          }          
        if(execution.getVariable('IDPosluga') == 'Одноразова винагорода жінкам, яким присвоєно почесне звання \"Мати-героїня\"')
          {
          Posluga ='Копія паспорту.&lt;br /&gt;Реквізити банківської установи для перерахування коштів.'
          }
        if(execution.getVariable('IDPosluga') == 'Тимчасова державна допомога дітям, батьки яких ухиляються від сплати аліментів, не мають можливості утримувати дитину або місце проживання їх невідоме')
          {
          Posluga ='Паспорт або інший документ, що посвідчує особу.&lt;br /&gt;Копія свідоцтва про народження дитини (з пред\'явленням оригіналу).&lt;br /&gt;Довідка про реєстрацію місця проживання (перебування) дитини.&lt;br /&gt;Довідка про реєстрацію місця проживання (перебування) одержувача.&lt;br /&gt;Залежно від підстав, на яких призначається тимчасова допомога, додатково подаються такі документи:&lt;br /&gt;Рішення суду (виконавчий лист про стягнення з одного з батьків аліментів на дитину).&lt;br /&gt;Довідка державної виконавчої служби, що підтверджує факт несплати аліментів одним з батьків протягом шести місяців, що передують місяцю звернення.&lt;br /&gt;Довідка відповідної установи про здійснення стосовно одного з батьків кримінального провадження або про його перебування на примусовому лікуванні, у місцях позбавлення волі, визнання його в установленому порядку недієздатним, а також перебування на строковій військовій службі.&lt;br /&gt;Повідомлення органу внутрішніх справ про те, що місце проживання (перебування) одного з батьків дитини не встановлено.&lt;br /&gt;Реквізити банківської установи для перерахування коштів.'
          }
        if(execution.getVariable('IDPosluga') == 'Довідка про отримання/неотримання усіх видів державних соціальних допомог')
          {
          Posluga ='Довідка про отримання (неотримання) соціальної допомоги видається на підставі письмової заяви або усної вимоги за умови пред\'явлення паспорта або іншого документа, який підтверджує особу та місце її проживання (реєстрації). Отримання іншою особою соціальної допомоги здійснюється лише у випадках, передбачених відповідними нормативно-правовими актами (на вимогу правоохоронних органів, органів опіки тощо).&lt;br /&gt;У разі, якщо заявник звертається з проханням надати довідку за телефоном, його вимога реєструється в Журналі видачі довідок та повідомлень, а заявникові повідомляється про час, коли можна отримати довідку.'
          }
        if(execution.getVariable('IDPosluga') == 'Довідка про забезпечення (не забезпечення) санаторно-курортним лікуванням')
          {
          Posluga ='Оригінал паспорту.'
          }
        if(execution.getVariable('IDPosluga') == 'Заява на виплату пільг у грошовій готівковій формі.')
          {
          Posluga ='Оригінал паспорту.&lt;br /&gt;Реквізити банківської установи для перерахування коштів.'
          }
