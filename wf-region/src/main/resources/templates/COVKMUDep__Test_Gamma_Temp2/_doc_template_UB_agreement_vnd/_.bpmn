    <scriptTask id="scripttask6" name="Table signers" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>var snID_Process_Activiti = execution.getProcessInstanceId();
var sLoginAuthor = execution.getVariable('sLoginAuthor');
var sLogin_Developer = taskForm.getValuesFromTableField(snID_Process_Activiti, 'sTable_Developer', 'sLogin_Developer'); 
var sLogin_DirDeveloper = taskForm.getValuesFromTableField(snID_Process_Activiti, 'sTable_Developer', 'sLogin_DirDeveloper'); 
var sName_DepDeveloper = taskForm.getValuesFromTableField(snID_Process_Activiti, 'sTable_Developer', 'sName_DepDeveloper'); 
var Accept1_agreement_vnd = oSubjectRoleService.getsEmployeesLoginsOfRoles('Accept1_agreement_vnd', null);
var NEW_fullArr = [];
var loginHead;

getUnique = function (arr) {
    var i = 0,
    current,
    length = arr.length,
    unique = [];
    for (; i &lt; length; i++) {
      current = arr[i];
      if (!~unique.indexOf(current)) {
        unique.push(current);
      }
    }
    return unique;
  };

for (var i = 0; i &lt; sLogin_Developer.length; i++) {
  loginHead = oDocumentService.getSubjectEmployeeParentLogin(sLogin_Developer[i]);
  if (loginHead != sLoginAuthor &amp;&amp; !~Accept1_agreement_vnd.indexOf(loginHead) &amp;&amp; loginHead != 'UB_IStaro1') {
    NEW_fullArr.push(loginHead);
  }
  if (sLogin_Developer[i] != sLoginAuthor &amp;&amp; !~Accept1_agreement_vnd.indexOf(sLogin_Developer[i])) {
    NEW_fullArr.push(sLogin_Developer[i]);
  }
}

for (var k = 0; k &lt; sLogin_DirDeveloper.length; k++) {
  if (sLogin_DirDeveloper[k] != sLoginAuthor &amp;&amp; !~Accept1_agreement_vnd.indexOf(sLogin_DirDeveloper[k])) {
    NEW_fullArr.push(sLogin_DirDeveloper[k]);
  }
}

execution.setVariable('sNEW_fullArr', getUnique(NEW_fullArr).toString().replace(/\[|\]|\s/g, ''));
execution.setVariable('sasName_DepDeveloper', getUnique(sName_DepDeveloper).toString().replace(/\[|\]/g, ''));</script>
    </scriptTask>
