    <scriptTask id="scripttask1" name="Отримання задач" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
//задачи
var snID_Process_Activiti = execution.getProcessInstanceId();

var aoProcessTasks = processSubjectTaskService.getProcessTasksVO(snID_Process_Activiti, null);
execution.setVariable('aoProcessTasks_str', aoProcessTasks.toString());
execution.setVariable('nCountTaskRes', aoProcessTasks.length.toString())

function formatFIO (key, index){
    sFormatName =key[index].oUserVO.sFirstName.toUpperCase() + ' ' +  key[index].oUserVO.sLastName.split(' ')[0].split('')[0] + '.' + key[index].oUserVO.sLastName.split(' ')[1].split('')[0] + '.';
    return sFormatName;
}

function formatFIOfromField (key){
    sFormatName = key.split(' ')[0].toUpperCase() + ' ' + key.split(' ')[1].split('')[0] + '. ' + key.split(' ')[2].split('')[0] + '.';
    return sFormatName;
}

function formatFIOforOthers (key, index){
    sFormatName = key[index].split(' ')[0].toUpperCase() + ' ' + key[index].split(' ')[1].split('')[0] + '. ' + key[index].split(' ')[2].split('')[0] + '.';
    return sFormatName;
}

function formatFIOforVisors (key, index){
    sFormatName = key[index].split(' ')[1] + ' ' + key[index].split(' ')[0].toUpperCase();
    return sFormatName;
}

function formatDate (date) {
	month = date.getMonth() + 1;
	day = date.getDate()
	if (month &lt; 10) {
		month = '0'+ month 
	} 
	if (day &lt; 10) {
		day = '0'+ day 
	}


	return day + '.' + month + '.' + date.getFullYear();
}

var aoTable_Task = [];
var asVisor = [];
var asVisorFormatted = [];
var asControllerFormatted = [];
var asCoordinatorFormatted = [];

if(aoProcessTasks.length &gt; 1){
	execution.setVariable('sCountTask', 'show');
}

for (i=0; i &lt; aoProcessTasks.length; i++) {
	oProcessTasks = aoProcessTasks[i];
	oRowTable = {};
	asController = [];
	asExecutor = [];

	for(k=0;k&lt;oProcessTasks.oProcessTaskSubjectsVO.aController.length;k++){
		asController.push(formatFIO(oProcessTasks.oProcessTaskSubjectsVO.aController, k))		
	};
	for(l=0;l&lt;oProcessTasks.oProcessTaskSubjectsVO.aExecutor.length;l++){
		asExecutor.push(formatFIO(oProcessTasks.oProcessTaskSubjectsVO.aExecutor, l))
	};
	// for(j=0;j&lt;oProcessTasks.oProcessTaskSubjectsVO.aVisor.length;j++){
	// 	asVisor.push(formatFIO(oProcessTasks.oProcessTaskSubjectsVO.aVisor, j))
	// };

	 sDateExecute1 = oProcessTasks.oProcessTaskSubjectsVO.aController[0].oDatePlan;
	 if(sDateExecute1){
	 sDate = sDateExecute1.toString().split('T')[0];
	 sDate = sDate.replace(/(\d+)-(\d+)-(\d+)/, '$3.$2.$1');
	// execution.setVariable('sDateExecute1', sDate);
	}else{
		nMillisecondsPlan = oProcessTasks.oProcessTaskSubjectsVO.aController[0].nDaysPlan * 24*60*60*1000;
		sDate = formatDate(new Date(Date.now() + nMillisecondsPlan))
	};

	 oRowTable.sHead = oProcessTasks.sHead;
	 oRowTable.sBody = oProcessTasks.sBody;
	 oRowTable.sDateExecute = sDate;
	 oRowTable.sController = asController.toString().replace(/\[|\]/g, '');
	 oRowTable.sExecutor = asExecutor.toString().replace(/\[|\]/g, '').replace(/,/g,'&lt;/br&gt;');
	 sVisor = asVisor.toString().replace(/\[|\]/g, '');

aoTable_Task.push(oRowTable);
//execution.setVariable('oProcessTasks', JSON.stringify(oRowTable));
}

//Получение До відома
var aNameUser = taskForm.getValuesFromTableField(snID_Process_Activiti, 'oTableVisors', 'sName_SubjectExecutor');
for(n=0; n&lt;aNameUser.length; n++){
	asVisorFormatted.push(formatFIOforOthers(aNameUser, n));
	execution.setVariable('asVisorFormatted', asVisorFormatted.toString());
}

// if(sNameDocumentVisors){
// 	sVisor = asVisorFormatted.toString().replace(/\[|\]/g, '') + ',' + sNameDocumentVisors;
// }else{
	sVisor = asVisorFormatted.toString().replace(/\[|\]/g, '').replace(/,/g,'&lt;/br&gt;');
// }

execution.setVariable('sVisors', sVisor);

//Отримання контролерів
var aNameController = taskForm.getValuesFromTableField(snID_Process_Activiti, 'oTableControlers', 'sName_SubjectExecutor');
for(n=0; n&lt;aNameController.length; n++){
	asControllerFormatted.push(formatFIOforOthers(aNameController, n));
	execution.setVariable('asControllerFormatted', asControllerFormatted.toString());
}

if(sNameDocumentControlers){
	sControlers = asControllerFormatted.toString().replace(/\[|\]/g, '').replace(/,/g,', ') + ', ' + sNameDocumentControlers;
}else{
	sControlers = asControllerFormatted.toString().replace(/\[|\]/g, '').replace(/,/g,', ');
}

execution.setVariable('sControlers', sControlers);

//Отримання координаторів
var aNameCoordinator = taskForm.getValuesFromTableField(snID_Process_Activiti, 'oTableCoordinators', 'sName_SubjectExecutor');
//var headDepName = execution.getVariable('sShortNameHeadDepart');

//if(headDepName) {
//	asCoordinatorFormatted.push(formatFIOfromField(headDepName));
//}

for(n=0; n&lt;aNameCoordinator.length; n++){
	asCoordinatorFormatted.push(formatFIOforOthers(aNameCoordinator, n));
	execution.setVariable('asCoordinatorFormatted', asCoordinatorFormatted.toString());
}

if(sNameDocumentCoordinators){
	sCoordinators = asCoordinatorFormatted.toString().replace(/\[|\]/g, '').replace(/,/g,', ') + ', ' + sNameDocumentCoordinators;
}else{
	sCoordinators = asCoordinatorFormatted.toString().replace(/\[|\]/g, '').replace(/,/g,', ')
}

execution.setVariable('sCoordinators', sCoordinators);

if(aoProcessTasks.length &gt; 0){
	processDocumentService.setProcessDocumentFieldTable(snID_Process_Activiti,  'oTableTask', JSON.stringify(aoTable_Task), false);
}
execution.setVariable('aoTable_Task', JSON.stringify(aoTable_Task));</script>
    </scriptTask>
    <scriptTask id="scripttask2" name="Зв'зка документів" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>//Связка одного документа, с поля
var sID_Order = execution.getVariable('sID_Document_Parent');
if(sID_Order){
	var sNum = sID_Order.split('-')[1];
	var processInstanceId_parent = sNum.substring(0, sNum.length - 1);
	var processInstanceId_child = execution.getProcessInstanceId();
	var nID_Process_Child = processDocumentService.getProcessDocument(processInstanceId_child).id;
	var nID_Process_Parent = processDocumentService.getProcessDocument(processInstanceId_parent).id;
	processDocumentTreeService.setProcessDocumentTree(+nID_Process_Parent, +nID_Process_Child);
}

//Связка нескольких документов, с таблицы
