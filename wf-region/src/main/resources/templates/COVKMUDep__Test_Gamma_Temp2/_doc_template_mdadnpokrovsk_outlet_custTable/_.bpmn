    <scriptTask id="scripttask4" name="вытягиваем логин + почты подписантов" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>var snID_Process_Activiti = execution.getProcessInstanceId()
var aDocumentStepLogin = documentStepService.getDocumentStepLogins(snID_Process_Activiti);

var asLoginSingers = [];
var asEmailSingers = [];
var sTypeSing = 'DirectExternal';

for (var i = 0; i &lt; aDocumentStepLogin .length; i++){
  var oDocumentStepLogin = aDocumentStepLogin[i];
  if(oDocumentStepLogin != null &amp;&amp; oDocumentStepLogin.oDocumentStepType != null  &amp;&amp; oDocumentStepLogin.oDocumentStepType.name != null &amp;&amp; oDocumentStepLogin.oDocumentStepType.name == sTypeSing){
    sLogin = oDocumentStepLogin.sLogin;
    //sEmail = oDocumentService.getSubjectEmployeeEmailFirst(sLogin);
    asEmail = subjectService.getSubjectContacts(sLogin, null, "Email", 1);
      if(asEmail.length &gt; 0){
        sEmail = asEmail[0];
        asEmailSingers.push(sEmail);
      }
      asLoginSingers.push(sLogin);      
    }
}
execution.setVariable('asLoginSingers', asLoginSingers.toString())
execution.setVariable('asEmailSingers', asEmailSingers.toString())</script>
