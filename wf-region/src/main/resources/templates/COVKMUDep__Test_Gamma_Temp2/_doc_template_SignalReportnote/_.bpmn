      <script>
//ID документу
var sID_Document = execution.getVariable('sID_Order_Document');
//ID поля з файлом CSV стрінгою
var sKey_FieldFile = 'oTest_File';
//кодування документа
var sEncodingType = 'windows-1251';
var oSetCSV = tableCustomService.getProcessTableVO_ByField_FromCSV( sID_Document,  sKey_FieldFile,  sEncodingType);
//об'єкт у джейсоні із файлом CSV преведеним до виду таблиці ProcessTableVO
var oSetCSV2 = tableCustomService. getProcessTableVO_ByField_FromCSV_JSON( sID_Document,  sKey_FieldFile,  sEncodingType);
//сетимо значення до змінної, аби перевірити данні
execution.setVariable('oSetCSV2',oSetCSV2)
//точка відліку, аби позбавитися від шапки таблиці
var from = oSetCSV2.search('"aasRowValue":');
var to = oSetCSV2.lenght;
//нова строка без шапки таблиці
var sPre_str1 = oSetCSV2.substring(from,to);
from = sPre_str1.search('[[');
to = sPre_str1.search('}"');
var sPre_str2 = sPre_str1.substring(from,to);
execution.setVariable('sPre_str2',sPre_str2)</script>
    </scriptTask>
