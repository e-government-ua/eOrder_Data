    <scriptTask id="scripttask5" name="Приведення у відповідність рядка із днями(роками) (рік-роки-років / день-дні-днів)" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>var s;
var sMore;
var y;
var yMore

if(execution.getVariable('asTypeVacation') == "sEnumVac_1") {
    var n = execution.getVariable('sDateDiff_1');
	if(n%10) {
        var numArr = n.toString().split('');
        var lastNum = numArr[numArr.length - 1]
        
        if(lastNum == 1) {
            s = n + " календарний день";
        } else if (lastNum &gt; 1 &amp;&amp; lastNum &lt; 5 ) {
            if (n == 12 || n == 13 || n == 14) {
                s = n + " календарних днів";
            } else {
                s = n + " календарні дні";
            }
        } else {
            s = n + " календарних днів";
        }
    } else {
        s = n + " календарних днів";
    }

    if(execution.getVariable('bAddCompensationType_1') == "yes") {
        var nMore = execution.getVariable('sDateDiff_1_More');
        if(nMore%10) {
            var numArrMore = nMore.toString().split('');
            var lastNumMore = numArrMore[numArrMore.length - 1]
            
            if(lastNumMore == 1) {
                sMore = nMore + " календарний день";
            } else if (lastNumMore &gt; 1 &amp;&amp; lastNumMore &lt; 5 ) {
                if (nMore == 12 || nMore == 13 || nMore == 14) {
                    sMore = nMore + " календарних днів";
                } else {
                    sMore = nMore + " календарні дні";
                }
            } else {
                sMore = nMore + " календарних днів";
            }
        } else {
            sMore = nMore + " календарних днів";
        }

        
        execution.setVariable('sDays_String_More', sMore);
    }

    execution.setVariable('sDays_String', s);
} else {
    var n = execution.getVariable('sDateDiff_2');

	if(n%10) {
        var numArr = n.toString().split('');
        var lastNum = numArr[numArr.length - 1]
        
        if(lastNum == 1) {
            s = n + " календарний день";
        } else if (lastNum &gt; 1 &amp;&amp; lastNum &lt; 5 ) {
            if (n == 12 || n == 13 || n == 14) {
                s = n + " календарних днів";
            } else {
                s = n + " календарні дні";
            }
        } else {
            s = n + " календарних днів";
        }
    } else {
        s = n + " календарних днів";
    }

    var nY = execution.getVariable('sExperience_2');

	if(nY%10) {
        var numArrY = nY.toString().split('');
        var lastNumY = numArrY[numArrY.length - 1]
        
        if(lastNumY == 1) {
            y = nY + " рік";
        } else if (lastNumY &gt; 1 &amp;&amp; lastNumY &lt; 5 ) {
            if (nY == 12 || nY == 13 || nY == 14) {
                y = nY + " років";
            } else {
                y = nY + " роки";
            }
        } else {
            y = nY + " років";
        }
    } else {
        y = nY + " років";
    }

    if(execution.getVariable('bAddCompensationType_2') == "yes") {
        var nMore = execution.getVariable('sDateDiff_2_More');
        if(nMore%10) {
            var numArrMore = nMore.toString().split('');
            var lastNumMore = numArrMore[numArrMore.length - 1]
            
            if(lastNumMore == 1) {
                sMore ="невикористаний " + nMore + " календарний день";
            } else if (lastNumMore &gt; 1 &amp;&amp; lastNumMore &lt; 5 ) {
                if (nMore == 12 || nMore == 13 || nMore == 14) {
                    sMore ="невикористані " +  nMore + " календарних днів";
                } else {
                    sMore ="невикористані " +  nMore + " календарні дні";
                }
            } else {
                sMore ="невикористані " +  nMore + " календарних днів";
            }
        } else {
            sMore ="невикористані " + nMore + " календарних днів";
        }

        var nYMore = execution.getVariable('sExperience_2_More');
    
        if(nYMore%10) {
            var numArrYMore = nYMore.toString().split('');
            var lastNumYMore = numArrYMore[numArrYMore.length - 1]
            
            if(lastNumYMore == 1) {
                yMore = nY + " рік";
            } else if (lastNumYMore &gt; 1 &amp;&amp; lastNumYMore &lt; 5 ) {
                if (nYMore == 12 || nYMore == 13 || nYMore == 14) {
                    yMore = nYMore + " років";
                } else {
                    yMore = nYMore + " роки";
                }
            } else {
                yMore = nYMore + " років";
            }
        } else {
            yMore = nYMore + " років";
        }
        
        
        execution.setVariable('sYears_String_More', yMore);
        execution.setVariable('sDays_String_More', sMore);
    }

    execution.setVariable('sDays_String', s);
    execution.setVariable('sYears_String', y);
}</script>
