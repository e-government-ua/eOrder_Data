    <scriptTask id="scripttask1" name="для старого счетчика" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>var asRecorder = execution.getVariable('asRecorder');
var indexRecorder = asRecorder.split('_')[1];
//номенклатура товариства
var arr = [921,922,923,924,925,926,927,928,929,930,931,932,902,910];
var num = arr[indexRecorder];

//Счетичик
var asRegister = execution.getVariable('asRegister');
var sCount = execution.getVariable('sCount');
var firstChar;
if(asRegister == 'sRegister_0'){
  //Первая буква фамилии
    var sFioName = execution.getVariable('sFioName');
    if(sFioName !== null){
       firstChar = sFioName.charAt();
    }
    var sCountFull = num + '-' + firstChar + sCount + '-з';
    execution.setVariable('sCountFull', sCountFull);
}

if(asRegister == 'sRegister_1'){
  var sCountFull = num + '-З' + sCount;
    execution.setVariable('sCountFull', sCountFull);
}</script>
    </scriptTask>
