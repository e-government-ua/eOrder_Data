} 

if (bOwnSavings == 'yes' || bOwnSavings == 'так') {
    sFinSource = sFinSource + " Власні заощадження готівки,";
} 

if (bDividends == 'yes' || bDividends == 'так') {
  sFinSource = sFinSource + " Дивіденди,";
} 

if (bHeritage == 'yes' || bHeritage == 'так') {
sFinSource = sFinSource + " Спадщина,";
}

