var bLoan = execution.getVariable('bLoan');
var bOtherIncome = execution.getVariable('bOtherIncome');
var sOtherOperationTypeIncome = execution.getVariable('sOtherOperationTypeIncome');

if (bSalary == 'yes' || bSalary == 'так') {
    sFinSource = sFinSource + " Заробітна плата, соціальні виплати,";
}

if (bPension == 'yes' || bPension == 'так') {
    sFinSource = sFinSource + " Пенсія,";
