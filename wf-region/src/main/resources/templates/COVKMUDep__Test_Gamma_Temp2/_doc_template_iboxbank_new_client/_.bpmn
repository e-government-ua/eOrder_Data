    sBankOper = sBankOper + " Операції з цінними паперами,";
} 

if (bBankMetal == 'yes' || bBankMetal == 'так') {
    sBankOper = sBankOper + " Операції з банківськими металами,";
}

if (bCredit == 'yes' || bCredit == 'так') {
    sBankOper = sBankOper + " Кредитні операції,";
} 

if (bDepposit == 'yes' || bDepposit == 'так') {
    sBankOper = sBankOper + " Депозитні операції,";
} 

if (bOther == 'yes' || bOther == 'так') {
    sBankOper = sBankOper + sOtherOperationType;
}

execution.setVariable('scli_bank_oper', sBankOper);

var bSalary = execution.getVariable('bSalary');
var bPension = execution.getVariable('bPension');
var bScholarship = execution.getVariable('bScholarship');
var bSPDIncome = execution.getVariable('bSPDIncome');
