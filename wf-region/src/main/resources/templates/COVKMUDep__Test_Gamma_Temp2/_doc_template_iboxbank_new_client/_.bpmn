      <script>function formatDate (date) {
	month = date.getMonth() + 1;
	day = date.getDate()
	if (month &lt; 10) {
		month = '0'+ month 
	} 
	if (day &lt; 10) {
		day = '0'+ day 
	}


	return day + '.' + month + '.' + date.getFullYear();
}

execution.setVariable('sBankVerifDate', formatDate(new Date()));</script>
