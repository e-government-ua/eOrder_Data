    <scriptTask id="scripttask10" name="Айди документа" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>var processInstanceId = execution.getProcessInstanceId();
var processInstanceIdToLong = parseFloat(processInstanceId);
var sID_Order= generalConfig.getOrderId_ByProcess(processInstanceIdToLong);
execution.setVariable('sID_Order', sID_Order);


var sKey_Private = execution.getVariable('sKey_Private');
if (sKey_Private == 'null') {sKey_Private = '_';}
execution.setVariable('sKey_Private', sKey_Private);

var oDateAt = execution.getVariable('oDateAt');
if (oDateAt == 'null') {oDateAt = '';}
execution.setVariable('oDateAt', oDateAt);

var sKey_Private_Outer = execution.getVariable('sKey_Private_Outer');
if (sKey_Private_Outer == 'null') {sKey_Private_Outer = '_';}
execution.setVariable('sKey_Private_Outer', sKey_Private_Outer);

var oDateAt_Outer = execution.getVariable('oDateAt_Outer');
if (oDateAt_Outer == 'null') {oDateAt_Outer = '';}
execution.setVariable('oDateAt_Outer', oDateAt_Outer);



//Для НБУ
var a = execution.getVariable('sFlag');
execution.setVariable('sFlag', a);

var c = execution.getVariable('sErrorText');
execution.setVariable('sErrorText', c);

var d = execution.getVariable('sID_Subject_DocumentSender');
execution.setVariable('sID_Subject_DocumentSender', d);

var e = execution.getVariable('sID_Request');
execution.setVariable('sID_Request', e);
var bIncome;
if(e != null){
  bIncome = true;
  execution.setVariable('asTypeIncome', 'sendNbu');
  execution.setVariable('sName_nomencl', 's_200011803-7');
}else{
  bIncome = false;
}
execution.setVariable('bIncome', bIncome);



//Variables for NBU to UB
var sCorresp = execution.getVariable('sCorresp');
var sName_Subject_DocumentSender = execution.getVariable('sName_Subject_DocumentSender');
execution.setVariable('sCorresp', sName_Subject_DocumentSender ? sName_Subject_DocumentSender : sCorresp);

var nPages_Document = execution.getVariable('nPages_Document');
var sQtyDop = execution.getVariable('sQtyDop');
execution.setVariable('sQtyDop', nPages_Document &gt; 0 ? nPages_Document : sQtyDop);

var sName_Document = execution.getVariable('sName_Document');
var sContent = execution.getVariable('sContent');
execution.setVariable('sContent', sName_Document ? sName_Document : sContent);

execution.setVariable('sNameAuthor', usersService.getUserFIObyLogin(execution.getVariable('sLoginAuthor')));</script>
    </scriptTask>
